#!/usr/bin/env python3
"""
Wrapper on /usr/bin/onionshare.

See netnsdrop module for a better understanding of how it works and its assumptions.
"""
import logging
import sys

from tailslib.netnsdrop import run


def main():
    run("/usr/bin/onionshare", "onionshare", "/usr/local/bin/onionshare", extra_args=sys.argv)


if __name__ == "__main__":
    logging.basicConfig(level=logging.INFO)
    main()
