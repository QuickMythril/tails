#!/usr/bin/env python3
import logging

from tailslib.netnsdrop import run


def main():
    run("/usr/bin/onionshare-gui", "onionshare", "/usr/local/bin/onionshare-gui", extra_env={
        "QT_ACCESSIBILITY": "1"
        })


if __name__ == "__main__":
    logging.basicConfig(level=logging.INFO)
    main()
