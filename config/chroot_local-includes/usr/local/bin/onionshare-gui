#!/usr/bin/env python3
"""
Wrapper on /usr/bin/onionshare-gui.

See netnsdrop module for a better understanding of how it works and its assumptions.
"""
import logging

from tailslib.netnsdrop import run


def main():
    run("/usr/bin/onionshare-gui", "onionshare", "/usr/local/bin/onionshare-gui")


if __name__ == "__main__":
    logging.basicConfig(level=logging.INFO)
    main()
